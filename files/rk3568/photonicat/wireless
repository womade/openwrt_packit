
config wifi-device 'radio0'
        option type 'mac80211'
        option path '3c0800000.pcie/pci0002:20/0002:20:00.0/0002:21:00.0'
        option channel '36'
        option band '5g'
        option htmode 'HE80'
        option country 'US'
        option legacy_rates '1'
        option mu_beamformer '0'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2'
        option key 'password'
        option ssid 'pcat_pcie_ax'

config wifi-device 'radio1'
        option type 'mac80211'
        option path 'platform/fe2c0000.mmc/mmc_host/mmc2/mmc2:0001/mmc2:0001:1'
        option band '5g'
        option htmode 'VHT80'
        option country 'US'
        option channel '149'
        option legacy_rates '1'
        option mu_beamformer '0'

config wifi-iface 'default_radio1'
        option device 'radio1'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2'
        option key 'password'
        option ssid 'pcat_sdio_ac'
